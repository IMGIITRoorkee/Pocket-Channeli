{
  "manifest_version": 2,

  "name": "Channel-i",
  "description": "This extension provides easy links to all the apps of Channel-i",
  "version": "1.0",

  "permissions": [
    "https://secure.flickr.com/",
    "*/channeli.in/hogwarts/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_name": "Channel-i",
    "icons": ["icon.png"]
  }
}
